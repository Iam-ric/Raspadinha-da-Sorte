<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".PrincipalActivity"
    tools:ignore="MissingClass">

    <ImageView
        android:id="@+id/iv_premium1"
        android:layout_width="200dp"
        android:layout_height="180dp"
        android:src="@drawable/img_win"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.985"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.442" />

    <ImageView
        android:id="@+id/iv_premium2"
        android:layout_width="200dp"
        android:layout_height="180dp"
        android:src="@drawable/img_win"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.918" />

    <ImageView
        android:id="@+id/iv_premium3"
        android:layout_width="200dp"
        android:layout_height="180dp"
        android:src="@drawable/img_win"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.985"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.918" />

    <ImageView
        android:id="@+id/iv_premium"
        android:layout_width="200dp"
        android:layout_height="180dp"
        android:src="@drawable/img_win"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.442" />

    <com.anupkumarpanwar.scratchview.ScratchView
        android:id="@+id/scratch_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:overlay_height="210dp"
        app:overlay_image="@drawable/scratch"
        app:overlay_width="180dp"
        app:tile_mode="CLAMP"
        app:layout_constraintBottom_toBottomOf="@id/iv_premium"
        app:layout_constraintEnd_toEndOf="@id/iv_premium"
        app:layout_constraintStart_toStartOf="@id/iv_premium"
        app:layout_constraintTop_toTopOf="@id/iv_premium"

        />

    <com.anupkumarpanwar.scratchview.ScratchView
        android:id="@+id/scratch_view1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:overlay_height="210dp"
        app:overlay_image="@drawable/scratch"
        app:overlay_width="180dp"
        app:tile_mode="CLAMP"
        app:layout_constraintBottom_toBottomOf="@id/iv_premium1"
        app:layout_constraintEnd_toEndOf="@id/iv_premium1"
        app:layout_constraintStart_toStartOf="@id/iv_premium1"
        app:layout_constraintTop_toTopOf="@id/iv_premium1"

        />

    <com.anupkumarpanwar.scratchview.ScratchView
        android:id="@+id/scratch_view2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:overlay_height="210dp"
        app:overlay_image="@drawable/scratch"
        app:overlay_width="180dp"
        app:tile_mode="CLAMP"
        app:layout_constraintBottom_toBottomOf="@id/iv_premium2"
        app:layout_constraintEnd_toEndOf="@id/iv_premium2"
        app:layout_constraintStart_toStartOf="@id/iv_premium2"
        app:layout_constraintTop_toTopOf="@id/iv_premium2"

        />

    <com.anupkumarpanwar.scratchview.ScratchView
        android:id="@+id/scratch_view3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:overlay_height="210dp"
        app:overlay_image="@drawable/scratch"
        app:overlay_width="180dp"
        app:tile_mode="CLAMP"
        app:layout_constraintBottom_toBottomOf="@id/iv_premium3"
        app:layout_constraintEnd_toEndOf="@id/iv_premium3"
        app:layout_constraintStart_toStartOf="@id/iv_premium3"
        app:layout_constraintTop_toTopOf="@id/iv_premium3"

        />


    <Button
        android:id="@+id/botaosair"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sair"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="320dp"
        tools:layout_editor_absoluteY="2dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.188" />

    <TextView
        android:id="@+id/subtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.005" />

</androidx.constraintlayout.widget.ConstraintLayout>